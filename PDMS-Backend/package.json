{
  "name": "pdms-backend",
  "version": "1.0.14",
  "description": "pdms-backend",
  "author": "",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development tsx watch --clear-screen=false app/index.ts | pino-pretty",
    "dev:win": "set NODE_ENV=development&& tsx watch --clear-screen=false app/index.ts | pino-pretty",
    "build": "tsup",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "biome check app/",
    "lint:fix": "biome check app/ --fix",
    "format": "biome format app/",
    "test": "vitest run"
  },
  "dependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/pdfkit": "^0.13.4",
    "@types/uuid": "^10.0.0",
    "argon2": "^0.40.3",
    "bcrypt": "^5.1.1",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "mime-types": "^2.1.35",
    "mongoose": "^8.5.2",
    "pdfkit": "^0.15.0",
    "pino-http": "^10.0.0",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.3.1",
    "uuid": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/config": "^3.3.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "lint-staged": "^15.2.2",
    "pino-pretty": "^11.0.0",
    "rimraf": "^6.0.0",
    "supertest": "^7.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --apply --no-errors-on-unmatched"
    ]
  },
  "tsup": {
    "entry": [
      "src",
      "!app/**/__tests__/**",
      "!app/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
